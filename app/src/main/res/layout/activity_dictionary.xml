                android:text="@string/m_my_n_n"
                android:textColor="#3FEE00"
                android:textSize="30sp" />

            <TextView
                android:id="@+id/a12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:outlineProvider="background"
